package PhilipsTravels;

public class Sid 
{
public static void main(String[] args) 
{
  String word = "Hi Sid 123";
   
  for(int i=0;i<word.length();i++)
  {     
      if(word.charAt(i)>='0')
          if(word.charAt(i)<='9')
          word=word.replaceAll("[a-z]","");   
  }
   word.replaceAll("[ ]", "")    ;
  System.out.println(word);
   
}
}
